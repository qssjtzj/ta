<!--性别-->		   
var sex = syscode.sex;
	ergodic( "sex" ,sex);
	function ergodic( classname , data ){
		var name = classname;
		var span = "";
		var len = data.length;
		for ( i = 0 ; i < len ;i++){
			var temp = "";
			temp = data[i].split(',');
			span += "<option value="+temp[0]+">"+temp[1]+"</option> "
		}
		console.log(span)
		$("."+classname+"").append( span );
	}
	
	
